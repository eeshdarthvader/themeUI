{
    "name": "themebook",
    "version": "0.0.16",
    "private": false,
    "scripts": {
        "lint": "eslint ./",
        "storybook": "start-storybook -p 6006",
        "storybook-build": "build-storybook -c .storybook -o storybook",
        "transpile": "babel src/components --extensions '.js' --out-dir packages --ignore 'src/**/*.stories.jsx','**/*.test.js' --copy-files --no-copy-ignored",
        "prepublishOnly": "rm -rf packages && yarn transpile"
    },
    "dependencies": {
        "react": "16.11.0",
        "react-dom": "16.11.0",
        "styled-components": "^4.4.1"
    },
    "devDependencies": {
        "eslint-plugin-prettier": "^3.1.3",
        "babel-plugin-styled-components": "^1.10.6",
        "@babel/preset-env": "^7.9.6",
        "@babel/cli": "^7.8.4",
        "@babel/core": "^7.9.6",
        "@babel/plugin-transform-runtime": "^7.9.6",
        "@babel/preset-react": "^7.9.4",
        "@storybook/addon-a11y": "5.3.18",
        "@storybook/addon-actions": "5.3.18",
        "@storybook/addon-backgrounds": "^5.3.18",
        "@storybook/addon-knobs": "5.3.18",
        "@storybook/addon-links": "5.3.18",
        "@storybook/addon-viewport": "5.3.18",
        "@storybook/addons": "5.3.18",
        "@storybook/react": "5.3.18",
        "babel-eslint": "^10.1.0",
        "babel-loader": "^8.1.0",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "eslint": "^6.8.0",
        "eslint-config-airbnb": "^18.1.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-import": "^2.20.2",
        "eslint-plugin-jsx-a11y": "^6.2.3",
        "eslint-plugin-react": "^7.19.0",
        "husky": ">=4",
        "lint-staged": ">=10",
        "path": "^0.12.7",
        "prettier": "^2.0.2",
        "prettierrc": "^0.0.0-5",
        "webpack": "^4.43.0",
        "webpack-cli": "^3.3.11"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.js": "eslint --cache --fix",
        "*.{js,css,md}": "prettier --write"
    }
}
